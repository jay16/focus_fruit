<%= simple_form_for(@site_config) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :domain %>
    <%= f.input :title %>
    <%= f.input :author %>
    <%= f.input :desc %>
    <%= f.input :weixin_token %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

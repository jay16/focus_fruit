<div class="span6 offset3">
  <table class="table table-hover">
    <thead>
      <tr>
	<th width="10%">图片</th>
	<th width="60%">名称</th>
	<th width="10%">大小</th>
	<th width="10%">编辑</th>
       </tr>
     </thead>
     <tbody>
      <% @pictures.each do |picture| %>
	<%= render partial: "pictures/picture", locals: { picture: picture } %>
      <% end if @pictures %>
    </tbody>
  </table>
</div>

<%= render "modal" %>
